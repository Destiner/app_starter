{
  "name": "starter-app",
  "version": "1.0.0",
  "description": "Starter Vue app",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "vue",
    "starter"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/starter-app.git"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "lint:prettier": "prettier -c \"src/**/*.{json,js,ts,vue}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\"",
    "typecheck": "npx tsc --noEmit && npx vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "3.4.3",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "3.4.15",
    "eslint": "8.56.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-vue": "9.19.2",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.3.3",
    "vite": "^3.2.8",
    "vue-tsc": "1.8.15"
  }
}
